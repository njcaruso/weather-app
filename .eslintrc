{
  "extends": "airbnb/legacy",
  "env": {
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "func-names": 0,
    "max-len": 0,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
    "vars-on-top": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "no-lonely-if": 0,
    "require-jsdoc": ["error", {
        "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": false,
            "ClassDeclaration": false,
            "ArrowFunctionExpression": false
        }
    }],
    "valid-jsdoc": 1,
  },
}
